<Shell
    x:Class="A_Connect.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:A_Connect.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="A_Connect">

    <ShellContent
        Title="Welcome!"
        ContentTemplate="{DataTemplate local:StartPage}"
        Route="StartPage" />
    
    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:Homepage}"
        Route="HomePage" />

    <ShellContent
        Title="Tutor Finder"
        ContentTemplate="{DataTemplate local:TutorFinderHomePage}"
        Route="TutorFinderHomePage" />

</Shell>
